{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/book/[id].js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/book/([0-9]+)",
        "dest": "/api/book/[id]"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }