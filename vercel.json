{
    "version": 2,
    "builds": [
      {
        "src": "api/book/[id].js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/book/([0-9]+)",
        "dest": "/api/book/[id]",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }